		<div class="single-product-slider">
			<div class="container">
				<div class="row d-flex">
					<%for(let i=0;i<data.length;i++){%>
						<div class="col-lg-3 col-md-6">
							<div class="single-product">
								<a href="/productDetail?id=<%=data[i]._id%>" class="img-wrap"> <img
									src="upload/product/<%=data[i].images[0]%>" alt="Product"> </a>		
								<div class="product-details">
									<h6>
										<%=data[i].name%>
									</h6>
									<div class="price">
										<h6>
											₹ <%=data[i].sale_price %>
										</h6>
										<h6 class="l-through">₹5000</h6>
									</div>

								</div>
							</div>
						</div>
						<%}%>
				</div>
			</div>

		</div>
